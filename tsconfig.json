{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "incremental": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true,
    "moduleResolution": "node"
  }
}
